<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="hotel_restaurant_guest_view_form" model="ir.ui.view">
        <field name="name">hotel.restaurant.guest.view.form</field>
        <field name="model">hotel.guest</field>
        <field name="inherit_id" ref="hotel_management_system.hotel_guest_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='bookingg_ids']" position="after">
                <field name="restaurant_order_ids"/>
            </xpath>
        </field>
    </record>

</odoo>