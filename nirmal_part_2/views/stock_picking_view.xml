<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_picking_view_partner_id" model="ir.ui.view">
        <field name="name">stock.picking.view.partner.id</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <button name="inventory_admin" type="object" class="btn-outline-primary"
                        string="acssess" />
            </xpath>
        </field>
    </record>

</odoo>